<property name="Tablet UI" active="true" image="/system/etc/paranoid/preferences/images/tablet.jpg">

        <description><![CDATA[
            160 DPI<br\>
            720px layout<br\>
            Tablet statusbar<br\>
            Tablet apps<br\>
            Softkeys off
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="720"/>
        <setting type="Hybrid" target="%system_default_layout" value="720"/>
        <setting type="Hybrid" target="%system_default_dpi" value="160"/>
        <setting type="Hybrid" target="%user_default_layout" value="720"/>
        <setting type="Hybrid" target="%user_default_dpi" value="160"/>
        <setting type="Hybrid" target="android.dpi" value="160"/>
        <setting type="Hybrid" target="android.layout" value="720"/>
        <setting type="Hybrid" target="com.paranoid.preferences.dpi" value="160"/>
        <setting type="Hybrid" target="com.paranoid.preferences.layout" value="720"/>
        <setting type="Hybrid" target="com.android.browser.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.browser.layout" value="720"/>
        <setting type="Hybrid" target="com.android.calculator2.dpi" value="128"/>
        <setting type="Hybrid" target="com.android.calculator2.layout" value="720"/>
        <setting type="Hybrid" target="com.android.settings.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.settings.layout" value="720"/>
        <setting type="Hybrid" target="com.android.contacts.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.contacts.layout" value="720"/>
        <setting type="Hybrid" target="com.android.calendar.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.calendar.layout" value="720"/>
        <setting type="Hybrid" target="com.android.gm.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.gm.layout" value="720"/>
        <setting type="Hybrid" target="com.android.talk.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.talk.layout" value="720"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.dpi" value="150"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.layout" value="600"/>
        <setting type="Android" target="max_notification_icons" value="4"/>
        <setting type="Android" target="nav_bar_status" value="0"/>

</property>
