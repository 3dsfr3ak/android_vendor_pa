<property name="Phablet UI" active="true" image="/system/etc/paranoid/preferences/images/phablet.jpg">

        <description><![CDATA[
            170 DPI<br\>
            600px layout<br\>
            Phablet statusbar<br\>
            Phablet apps<br\>
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="600"/>
        <setting type="Hybrid" target="%system_default_layout" value="600"/>
        <setting type="Hybrid" target="%system_default_dpi" value="170"/>
        <setting type="Hybrid" target="%user_default_layout" value="600"/>
        <setting type="Hybrid" target="%user_default_dpi" value="170"/>
        <setting type="Hybrid" target="android.dpi" value="170"/>
        <setting type="Hybrid" target="android.layout" value="600"/>
        <setting type="Hybrid" target="com.paranoid.preferences.dpi" value="170"/>
        <setting type="Hybrid" target="com.paranoid.preferences.layout" value="600"/>
        <setting type="Hybrid" target="com.android.browser.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.browser.layout" value="600"/>
        <setting type="Hybrid" target="com.android.calculator2.dpi" value="128"/>
        <setting type="Hybrid" target="com.android.calculator2.layout" value="600"/>
        <setting type="Hybrid" target="com.android.settings.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.settings.layout" value="600"/>
        <setting type="Hybrid" target="com.android.contacts.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.contacts.layout" value="600"/>
        <setting type="Hybrid" target="com.android.calendar.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.calendar.layout" value="600"/>
        <setting type="Hybrid" target="com.android.gm.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.gm.layout" value="600"/>
        <setting type="Hybrid" target="com.android.talk.dpi" value="170"/>
        <setting type="Hybrid" target="com.android.talk.layout" value="600"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.dpi" value="190"/>
        <setting type="Hybrid" target="com.cyanogenmod.trebuchet.layout" value="360"/>

</property>
